  });

  /* RAQ right side product management section JS start */
  $(document).on('click', '#request-quote-summary-button', function(){
    $('#request-quote-details').toggle();
    if ($('#request-quote-summary-icon').hasClass('action-cart-arrow')) {
      $('#request-quote-summary-icon').removeClass('action-cart-arrow');
    } else {
      $('#request-quote-summary-icon').addClass('action-cart-arrow');
    }
    let requestQuoteSummaryButtonTxt = $.trim($('#request-quote-summary-button-txt').text());

    if (requestQuoteSummaryButtonTxt == 'Show quote summary') {
      $('#request-quote-summary-button-txt').text('Hide quote summary');
    } else {
      $('#request-quote-summary-button-txt').text('Show quote summary');
    }
  });

  $(document).on('click', '.raq-line-item-quantity-selector__button, .raq-line-item-remove-button', function(){
    let raqItemLine = parseInt($(this).attr('data-line'));
    let raqItemDataAction = $(this).attr('data-action');
    let targetRaqItemQuantity = parseInt($(`#raq-line-item-quantity-${raqItemLine}`).val());

    let itemQuantity = 1;

    if (raqItemDataAction.toLowerCase() === 'decrease-quantity-quote') {
        itemQuantity = targetRaqItemQuantity - 1;
    } else if (raqItemDataAction.toLowerCase() === 'increase-quantity-quote') {
        itemQuantity = targetRaqItemQuantity + 1;
    } else if (raqItemDataAction.toLowerCase() === 'remove-quote') {
        itemQuantity = 0;
    }

    raqLineItemQuanityUpdation(raqItemLine, itemQuantity);
  });

  function raqLineItemQuanityUpdation(raqItemLine, itemQuantity)
  {
    document.dispatchEvent(new CustomEvent('theme:loading:start'));
    fetch("".concat(window.routes.cartChangeUrl, ".js"), {
      body: JSON.stringify({
        line: raqItemLine,
        quantity: itemQuantity
      }),
      credentials: 'same-origin',
      method: 'POST',
      headers: {
        'Content-Type': 'application/json',
        'X-Requested-With': 'XMLHttpRequest'
      }
    }).then(function (cart) {
      cart.json().then(function (content) {
        if (!parseInt(content.item_count)) {
          document.documentElement.dispatchEvent(new CustomEvent('cart:refresh', {
             bubbles: true
          }));
          window.location.reload();
        } else if (!parseInt(itemQuantity)) {
          window.location.reload();
        }

        if (parseInt(itemQuantity)) {
          $(`#raq-line-item-thumbnail-quantity-${raqItemLine}`).html(itemQuantity);
          $(`#raq-line-item-quantity-${raqItemLine}`).val(itemQuantity);
          $(`#raq-line-item-hidden-quantity-${raqItemLine}`).html(itemQuantity);

          // Highlight: Fetch Unit Price and Calculate Total Price
          let unitPrice = parseFloat($(`#raq-line-item-price-${raqItemLine}`).data('unit-price')) / 100; // Divide by 100 to convert from cents to dollars
          let totalPrice = unitPrice * itemQuantity;
          let raqCurrency = content.currency || 'USD';
          let raqCurrencyFormat = window.theme.currencyCodeEnabled ? window.theme.moneyWithCurrencyFormat : window.theme.moneyFormat;
  
          // Highlight: Format Total Price
          let formattedTotalPrice = CurrencyClass.formatMoney(totalPrice * 100, raqCurrencyFormat); // Multiply by 100 to convert back to cents for formatting
  
          // Highlight: Update HTML with Formatted Total Price
          $(`#raq-line-item-price-${raqItemLine}`).html(formattedTotalPrice);
        } else {
          $(`#raq-line-item-div-${raqItemLine}`).remove();
        }

        let raqCurrency = 'USD';
        if (content.currency) {
          raqCurrency = content.currency;
        }

        if (!parseInt(content.item_count)) {
          let raqCurrencyFormat = window.theme.currencyCodeEnabled ? window.theme.moneyWithCurrencyFormat : window.theme.moneyFormat;
          let default_price = '0.00';
          let default_formatted_price = CurrencyClass.formatMoney(default_price, raqCurrencyFormat);

          $(`#raq-line-item-subtotal`).html(default_formatted_price);
          $(`#raq-line-item-total`).html(`${raqCurrency} ${default_formatted_price}`);
          $(`#request-quote-summary-button-total-amount`).html(`${raqCurrency} ${default_formatted_price}`);
        }

        if (content.items_subtotal_price) {
          let subtotal_price = parseFloat(parseFloat(content.items_subtotal_price) / 100).toFixed(2);
          let raqCurrencyFormat = window.theme.currencyCodeEnabled ? window.theme.moneyWithCurrencyFormat : window.theme.moneyFormat;
          let formatted_subtotal_price = CurrencyClass.formatMoney(subtotal_price, raqCurrencyFormat);

          $(`#raq-line-item-subtotal`).html(formatted_subtotal_price);
          $(`#raq-line-item-total`).html(`${raqCurrency} ${formatted_subtotal_price}`);
          $(`#request-quote-summary-button-total-amount`).html(`${raqCurrency} ${formatted_subtotal_price}`);
        }

        //updateRaqLineItemUI(raqItemLine, itemQuantity);

        document.documentElement.dispatchEvent(new CustomEvent('cart:refresh', {
          bubbles: true
        }));

        document.dispatchEvent(new CustomEvent('theme:loading:end'));
      });
    }).catch(function (error) {
      resetRaqLineItemQuantityToOld(raqItemLine);
      console.log(error);
      document.dispatchEvent(new CustomEvent('theme:loading:end'));
    });
  }

  function updateRaqLineItemUI(raqItemLine, itemQuantity)
  {
    if ($(`#raq-line-item-quantity-decrease-btn-${raqItemLine}`).length) {
      if (parseInt(itemQuantity) == 1) {
        $(`#raq-line-item-quantity-decrease-btn-${raqItemLine}`).html(getRaqLineItemDeleteIcon());
      } else {
        $(`#raq-line-item-quantity-decrease-btn-${raqItemLine}`).html(getRaqLineItemDecreaseIcon());
      }
    }
  }

  function getRaqLineItemDeleteIcon()
