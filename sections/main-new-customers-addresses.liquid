<section class="site-i-cart req-qt-all" data-section-id="{{ section.id }}" data-section-type="address">
  <!--Contener Page Wrap Start-->
  <div class="container">
    <span id="user-login" data-user="{% if customer %}true{% else %}false{% endif %}"></span> 
  
    <!--Login Section Template Start-->
    <div class="log-temp custom-log-temp custon-quate" id="custom-log-temp" style="margin-bottom: 50px;">
      {%- if shop.customer_accounts_enabled -%}
        {%- if customer -%} 
          <div class="log-temp-cell custom-log-temp-cell with-custom-bradcrumb reql">
            <div class="">
              {% unless customer %}
                <div class="snak-bar">
                  <span>
                    <img src="https://cdn.shopify.com/s/files/1/2372/2681/files/atention-icon-1.png?v=1678445130" alt="" loading="lazy" />
                  </span>
                  In order to submit your quote request you will need to have an account.
                </div> 
              {% endunless %}
              
              <div style="float: left; width:100%">
                <div class="quate-new-form">
                  <div class="form__header">
                    <span class="header__action-item-title custom-left" style="font-size:20px;font-weight: 600;color: #4d4d4f; line-height:30px;">Hi {{ customer.first_name | truncate: 20 }}</span>
                  </div>
                  <div class="custom-contact-information custom-data-requirements">
                    <div class="custom-address-list__item">
                      <div class="custom-address-list__item-top">
                        <p class="heading h5">Contact Information</p>
                        <a href="/account#profile">
                          <button type="button" class="link link--accented btn-edit edit-icon">
                            {%- render 'icon', icon: 'edit-icon' -%}
                          </button>
                        </a>
                      </div>
                      <div class="address-list__item-info" style="text-align:left;">
                        <strong><span>{{ customer.first_name }} {{ customer.last_name }}</span></strong><br/>
                        <span>{{ customer.email }}</span><br/>
                        <span>{{ customer.phone }} </span>
                        <span>{{ customer_address.company }}</span><br/>
                      </div>
                    </div>
                  </div>
                  <form class="custom-form" method="post" action="{{ routes.cart_url }}" novalidate="novalidate" data-item-count="{{ cart.item_count }}">
                    <div class="custom-data-requirements">
                      <div class="form-group" id="contact-data">
