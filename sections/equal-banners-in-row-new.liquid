
<style>
  #section-{{section.id}}{
    margin-top: {{ section.settings.section_margin_top }}px!important;
    margin-bottom: {{ section.settings.section_margin_bottom }}px!important;
  }
 

  .equal-banner-row .image-row{
      display: flex;
      flex-wrap: wrap;
      gap: 10px;
  }
  .equal-banner-row .image-row .image-row-item{
      width: 100%;
      display: flex;
      flex-direction: row;
      gap: 10px;
      overflow-x: hidden;
      -ms-overflow-style: none;
      scrollbar-width: none;
  }
  .equal-banner-row .image-row .image-row-item::-webkit-scrollbar{
    display: none;
  }
  .equal-banner-row .image-row .image-row-item .row-main-link{
      display: block;
      position: relative;
      width: 100%;
      border-radius: 10px;
      overflow: hidden;
  }
  .equal-banner-row .image-row .image-row-item img.background-image{
      border-radius: 10px;
      width: 100%;
  }
  .equal-banner-row .image-row .image-row-item.full-width-banner .image-row-item-child{
    width: 100%;
  }
  .equal-banner-row .image-row .row-main-link .label{
    position: absolute;
    top: 0;
}
.equal-banner-row .image-row .row-main-link .label span.new{
    width: 75px;
    height: 25px;
    display: flex;
    justify-content: center;
    align-items: center;
    background-color: #0593d2;
    color: #FFFFFF;
    border-bottom-left-radius: 5px;
    border-bottom-right-radius: 5px;
    padding-top: 3px;
    font-size: 15px;
    font-weight: 800;
}
.equal-banner-row .image-row .row-main-link .label span.renewed{
    height: 25px;
    display: flex;
    justify-content: center;
    align-items: center;
    background-color: #08a901;
    color: #FFFFFF;
    border-bottom-left-radius: 5px;
    border-bottom-right-radius: 5px;
    padding-top: 3px;
    font-size: 15px;
    font-weight: 800;
}
.equal-banner-row .image-row .row-main-link .label .renewed-container{
    padding: 0px 10px;
    gap: 6px;
    display: flex;
    justify-content: center;
    align-items: center;
    background-color: #08a901;
    border-bottom-left-radius: 5px;
    border-bottom-right-radius: 5px;
    height: 25px;
    overflow: hidden;
}
.equal-banner-row .image-row .row-main-link .custom-secondry-image, .equal-banner-row .image-row .row-main-link .custom-second-image, .equal-banner-row .image-row .row-main-link .custom-third-image {
  position: absolute;
}
.equal-banner-row .image-row .row-main-link .custom-secondry-image img, .equal-banner-row .image-row .row-main-link .custom-second-image img, .equal-banner-row .image-row .row-main-link .custom-third-image img{
  width: 100%;
}
.equal-banner-row .image-row .row-main-link .heading-container{
  position: absolute;
  display: flex;
  flex-direction: column;
  width: 100%;
}
.equal-banner-row .image-row .row-main-link .free-shipping-container{
  position: absolute;
  display: flex;
  justify-content: center;
  align-items: center;
}
.equal-banner-row .image-row .row-main-link .free-shipping-container p{
  margin-bottom: 0px;
  font-family: "Encode Sans";
  font-style: normal;
  font-weight: 500;
  line-height: normal;
}
.equal-banner-row .image-row .row-main-link .heading-container h2{
  color: inherit;
  font-family: "Encode Sans";
  font-size: inherit;
  font-style: normal;
  font-weight: 700;
  line-height: normal;
  margin-bottom: 0px;
}
.equal-banner-row .image-row .row-main-link .heading-container span{
  color: inherit;
  font-family: "Encode Sans";
  font-style: normal;
  font-weight: 500;
  line-height: normal;
}
.equal-banner-row .image-row .row-main-link .button-container span.shop-now-btn{
  border-radius: 5px;
  font-family: "Encode Sans";
  font-size: 12px;
  font-style: normal;
  font-weight: 600;
  line-height: normal;
  display: flex;
  justify-content: center;
  align-items: center;
}
.equal-banner-row .image-row .row-main-link .button-container{
  position: absolute;
}
.equal-banner-row .image-row .row-main-link .price-container{
  position: absolute;
}
.equal-banner-row .image-row .row-main-link .price-container .compare-price{
  text-align: right;
  font-family: "Encode Sans";
  font-style: normal;
  font-weight: 400;
  line-height: normal;
}
.equal-banner-row .image-row .row-main-link .price-container .compare-price span{
  position: relative;
}
.equal-banner-row .image-row .row-main-link .price-container .compare-price span::before{
  content: "";
  height: 1px;
  width: 100%;
  position: absolute;
  top: 50%;
  left: 0px;
}
.equal-banner-row .image-row .row-main-link .price-container .compare-price span.tilted-strikethrough::before{
  transform: rotate(-10deg);
}
.equal-banner-row .image-row .row-main-link .price-container .main-price{
  font-family: "Encode Sans";
  font-style: normal;
  font-weight: 800;
  line-height: normal;
  display: flex;
  margin-top: -4px;
  direction: ltr;
}
.equal-banner-row .image-row .row-main-link .price-container .main-price sup{
  font-size: 16px!important;
  margin-top: 4px;
  display: inline-block;
}
/*.equal-banner-row .image-row .row-main-link .price-container .main-price .currency-code-container{
  display: flex;
  flex-direction: column;
  margin-left: 2px;
}*/
.equal-banner-row .image-row .row-main-link .price-container .main-price .currency-code{
  font-size: 16px!important;
  margin-left: -4px;
}
.equal-banner-row .image-row .image-row-item-child{
  width: calc(33.33% - 7px);
}
.equal-banner-row .image-row .item-for-2 .image-row-item-child{
  width: calc(50% - 5px);
}
.image-row-item .placeholder-svg{
  aspect-ratio: 23/18;
}
.image-row-item.full-width-banner .placeholder-svg, .item-for-2.image-slider-for-2 .placeholder-svg{
  max-height: 360px;
}

.equal-banner-row span.price-symbol{
  font-weight: inherit;
  margin-right: 5px;
}

.equal-banner-row span.price-symbol.second{
  margin-left: -7px;
  margin-right: 1px;
}
.equal-banner-row .content-for-mobile{
  display: none!important;
}
.equal-banner-row .image-row .row-main-link .price-container .main-price .manage-main-price{
  display: flex;
}

/* .equal-banner-row #showMargin{
  margin-right: 30px;
} */

.equal-banner-row .heading-container span ul li::marker{
  margin: 0px;
}

.equal-banner-row .content-for-mobile{
  display: none!important;
}

.equal-banner-row .image-row .image-row-item.full-width-banner .image-row-item-child{
  flex: 0 0 100%;
}
.equal-banner-row .main-cont{
  padding: 0px!important;
}
.equal-banner-row .image-row .image-row-item{
  padding: 0 var(--desktop-container-gutter);
}

@media (min-width: 1150px) and (max-width: 1340px) {
  .equal-banner-row .image-row .row-main-link .price-container .main-price sup{
    font-size: 14px!important;
    margin-top: 2px;
  }
  .equal-banner-row .image-row .row-main-link .price-container .main-price .currency-code{
    font-size: 14px!important;
  }
  .equal-banner-row .image-row .row-main-link .price-container .compare-price{
    font-size: 14px;
  }
}
@media (min-width: 1025px) and (max-width: 1250px){
  .equal-banner-row span.price-symbol{
    margin-right: 2px;                 
  }
}
@media only screen and (max-width: 1024px) {
  .equal-banner-row .image-row .image-row-item.full-width-banner .image-row-column{
    padding-right: 0px;
  }

  .equal-banner-row .image-row .image-row-item{
    overflow-x: auto;
  }

  .equal-banner-row .image-row .image-row-item .image-row-item-child{
    flex: 0 0 45%;
  }
  
  .equal-banner-row .image-row{
    gap: 1.6px;
  }
  
  .equal-banner-row .image-row .image-row-item.image-slider-for-2 .image-row-item-child {
        flex: 0 0 48%;
    }
 
}

@media only screen and (max-width: 991px){
  .equal-banner-row .image-row .image-row-item .image-row-item-child {
        flex: 0 0 49%;
    }
    .equal-banner-row .image-row .image-row-item.image-slider-for-2 .image-row-item-child {
        flex: 0 0 48%;
    }
}

@media only screen and (max-width: 900px){
  .equal-banner-row .content-for-mobile{
    display: flex!important;
  }
  .equal-banner-row .content-for-desktop{
    display: none!important;
  }
}
@media only screen and (max-width: 768px) {
  #section-{{section.id}}{
    margin-top: {{ section.settings.section_margin_top_mobile }}px!important;
    margin-bottom: {{ section.settings.section_margin_bottom_mobile }}px!important;
  }
}
