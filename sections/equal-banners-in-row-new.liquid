    .equal-banner-row .image-row .image-row-item.image-slider-for-2 .image-row-item-child {
      flex: 0 0 65%;
  }
}
@media only screen and (max-width: 641px){
  .equal-banner-row .image-row .image-row-item{
    padding: 0 calc(var(--mobile-container-gutter) - 10px);
  }
  html[lang="ar"] .equal-banner-row .image-row .image-row-item.small-banner-last {
    padding-right: calc(var(--mobile-container-gutter) - 12px);
    padding-left: calc(var(--mobile-container-gutter) - 10px);
    
}
}

@media only screen and (max-width: 480px) {
    .equal-banner-row .image-row .image-row-item .image-row-item-child {
        flex: 0 0 85%;
    }
    .equal-banner-row .image-row .image-row-item.image-slider-for-2 .image-row-item-child {
      flex: 0 0 85%;
  }
}

@media only screen and (max-width: 400px){
  /*.equal-banner-row .image-row .row-main-link .price-container .main-price{
    font-size: 16px;
  }
  .equal-banner-row .image-row .row-main-link .price-container .compare-price{
    font-size: 10px;
  }*/
}
</style>

<section id="section-{{section.id}}" class="equal-banner-row section">
  <div class="container main-cont">
     
      {% for block in section.blocks %}
          {%- capture block_capture -%}
              <div class="image-row-column {{ forloop.index }}">

                {% assign main_link = block.settings.link %}
                    {% assign code = request.locale.iso_code %}

                    {% if code != 'en' %}
                      {% assign monitors_domain = 'monitors.com' %}

                      {% if main_link contains monitors_domain %}
                          {% if code == 'pt-PT' %}
                            {% capture new_domain %}{{ monitors_domain }}/pt{% endcapture %}
                          {% else %}
                            {% capture new_domain %}{{ monitors_domain }}/{{ code }}{% endcapture %}
                          {% endif %}
                          {% capture new_link %}
                          {{ main_link | replace: monitors_domain, new_domain }}
                        {% endcapture %}
                      {% else %}
                        {% assign new_link = main_link %}
                      {% endif %}
                    {% else %}
                      {% assign new_link = main_link %}
                    {% endif %}


                  <a href="{{ new_link }}" class="row-main-link" id="block-{{block.id}}">
                      {% if block.settings.show_label %}
                          <div class="label">
                              {% if block.settings.label_type == "new" %}
                                  <span class="new">NEW</span>
                              {% else %}
                                  <div class="renewed-container">
                                      <img src="https://cdn.shopify.com/s/files/1/2372/2681/files/ICON_LEAF_2.png?v=1730103540" width="13px" height="18px" alt="Renewed icon" loading="lazy">
                                      <span class="renewed">
                                        RENEWED
                                      </span>
                                  </div>
                              {% endif %}
                          </div>
                      {% endif %}
                      {%  if block.settings.background_type == 'image' %}
                        {% if block.settings.image != blank %}
                            {{ block.settings.image | image_url: width: block.settings.image.width | image_tag: sizes: 'min(100vw, 560px)', widths: '200,300,400,500,600,800,1000,1200,1400,1600', class: 'background-image' }}
                        {% else %}
                            {{ 'image' | placeholder_svg_tag: 'placeholder-svg' }}
                        {% endif %}
                      {%  endif %}

                      {%  if block.settings.background_type == 'gradient' %}
                      <div class="background-gradient"></div>
                      {% endif %}

                      {% comment %} for secondry (first) image desktop {% endcomment %}
                      {%- if block.settings.secondary_image != blank -%}
                        <div class="custom-secondry-image">
                          {{ block.settings.secondary_image | image_url: width: block.settings.secondary_image.width | image_tag: loading: 'lazy', sizes: 'min(100vw, 560px)', widths: '200,300,400,500,600,800,1000,1200,1400,1600' }}
                        </div>
                      {% endif %}
                      {% comment %} end secondary (first) image  {% endcomment %}

                      {% comment %} for second image desktop {% endcomment %}
                      {%- if block.settings.second_image != blank -%}
                        <div class="custom-second-image">
                          {{ block.settings.second_image | image_url: width: block.settings.second_image.width | image_tag: loading: 'lazy', sizes: 'min(100vw, 560px)', widths: '200,300,400,500,600,800,1000,1200,1400,1600' }}
                        </div>
                      {% endif %}
                      {% comment %} end second image  {% endcomment %}

                      {% comment %} for third image desktop {% endcomment %}
                      {%- if block.settings.third_image != blank -%}
                        <div class="custom-third-image">
                          {{ block.settings.third_image | image_url: width: block.settings.third_image.width | image_tag: loading: 'lazy', sizes: 'min(100vw, 560px)', widths: '200,300,400,500,600,800,1000,1200,1400,1600' }}
                        </div>
                      {% endif %}
                      {% comment %} end third image  {% endcomment %}

                      {% comment %} code for free shipping container {% endcomment %}

                      {% if block.settings.free_shipping_image != blank and block.settings.free_shipping_text != blank %}
                        <div class="free-shipping-container">
                          <img src="{{ block.settings.free_shipping_image | image_url }}" alt="Free shipping icon" loading="lazy">
                          <p>{{ block.settings.free_shipping_text }}</p>
                        </div>
                      {% endif %}

                      {% comment %} end code free shipping container {% endcomment %}


                      {% comment %} code for title  {% endcomment %}
                      {% if block.settings.title != blank %}
                        <div class="heading-container content-for-desktop">
                          <h2>{{ block.settings.title | newline_to_br }}</h2>
                          <span>{{ block.settings.content | newline_to_br }}</span>
                        </div>
                    {% endif %}
                    {% if block.settings.title_mobile != blank %}
                      <div class="heading-container content-for-mobile">
                        <h2>{{ block.settings.title_mobile | newline_to_br }}</h2>
                        <span>{{ block.settings.content_mobile | newline_to_br }}</span>
                      </div>
                  {% endif %}
                    {% comment %} end title code  {% endcomment %}

                    {% comment %} code for shop now button  {% endcomment %}
                    {% if block.settings.button_text != blank %}
                      <div class="button-container">
                        <span class="shop-now-btn">
                          {{ block.settings.button_text }}
                        </span>
                      </div>
                    {% endif %}

                    {% comment %} end shop now code  {% endcomment %}

                    {% comment %} code for price container {% endcomment %}
                    
                    {% assign product_handle = block.settings.featured_product %}
                    {% assign product = all_products[product_handle] %}
                    {% assign availableVariants = product.variants | where: 'available', true %}

                    {% assign url_query = block.settings.link %}
                    {% if url_query contains 'variant=' %}
                      {% assign variant_id = url_query | split: 'variant=' | last | split: '&' | first %}
                      {% assign variant_id_number = variant_id | times: 1 %}
                      {% for variant in availableVariants %}
                        {% if variant.id == variant_id_number %}
                          {% assign firstAvailableVariant = variant %}
                        {% endif %}
                      {% endfor %}
                    {% else %}
                      {% assign firstAvailableVariant = availableVariants.first %}
                    {% endif %}

                    {% assign price = firstAvailableVariant.price %}


                    {% if cart.currency.iso_code == 'EUR' or cart.currency.iso_code == 'QAR'  or cart.currency.iso_code == 'RON' or cart.currency.iso_code == 'PLN' or cart.currency.iso_code == 'HUF' or cart.currency.iso_code == 'DKK' or cart.currency.iso_code == 'CZK' or cart.currency.iso_code == 'BAM' %}
                      {% assign mod_price_first = price | money_without_currency | split: ',' | first  %}
                      {% assign mod_price_last = price | money_without_currency %}
                      {% if mod_price_last contains ',' %}
                        {% assign mod_price_last = mod_price_last | split: ',' | last %}
                      {% else %}
                        {% assign mod_price_last = '' %}
                      {% endif %}
                    {% else %}
                      {% assign mod_price_first = price | money_without_currency | split: '.' | first  %}
                      {% assign mod_price_last = price | money_without_currency %}
                      {% if mod_price_last contains '.' %}
                        {% assign mod_price_last = mod_price_last | split: '.' | last %}
                      {% else %}
                        {% assign mod_price_last = '' %}
                      {% endif %}
                    {% endif %}
                    {% if mod_price_last == '00' %}
                      {% assign mod_price_last = '' %}
                    {% endif %}

                    {% if block.settings.featured_product != blank %}
                      <div class="price-container {{ product_handle }}">
                          <div class="compare-price">
                            <span {% if cart.currency.iso_code != 'USD' %}id="showMargin"{% endif %} {% if block.settings.compare_price_strike == 'tilted' %} class="tilted-strikethrough" {% endif %}>
                              {{ firstAvailableVariant.compare_at_price | money_without_trailing_zeros }}
                            </span>
                            {% if cart.currency.iso_code == 'AED' or cart.currency.iso_code == 'SAR' or cart.currency.iso_code == 'QAR' or cart.currency.iso_code == 'EGP' %}
                              <span class="price-symbol compare-second" style="visibility:hidden;">{{ cart.currency.symbol }}</span>
                            {% endif %}
                            {% if cart.currency.iso_code != 'USD' and cart.currency.iso_code != 'SAR' %}<span class="currency-code" style="visibility:hidden;">{{ cart.currency.iso_code }}</span>{% endif %}
                          </div>
                          <div class="main-price">
                            {% unless cart.currency.iso_code == 'AED' or cart.currency.iso_code == 'SAR' or cart.currency.iso_code == 'QAR' or cart.currency.iso_code == 'EGP' %}
                             <span class="price-symbol">{{ cart.currency.symbol }}</span>
                            {% endunless %}
                            <span class="manage-main-price">
                              {% comment %} {% if cart.currency.iso_code != 'USD' %}<span class="currency-code second">{{ cart.currency.iso_code }}</span>{% endif %} {% endcomment %}
                              <span>{{ mod_price_first }}</span>
                            <span class="currency-code-container">
                              <sup class="">{{ mod_price_last }}</sup>
                              {% comment %} <span class="price-symbol second">{{ cart.currency.symbol }}</span> {% endcomment %}
                              {% if cart.currency.iso_code == 'AED' or cart.currency.iso_code == 'SAR' or cart.currency.iso_code == 'QAR' or cart.currency.iso_code == 'EGP' %}
                                <span class="price-symbol second">{{ cart.currency.symbol }}</span>
                               {% endif %}
                              {% if cart.currency.iso_code != 'USD' and cart.currency.iso_code != 'SAR' %}<span class="currency-code">{% if cart.currency.iso_code == 'TRY' %} TL{% else %}{{ cart.currency.iso_code }}{% endif %}</span>{% endif %}
                            </span>
                            </span>
                          </div>
                      </div>
                    {% endif %}
                    
                    {% comment %} end price container code  {% endcomment %}

                  </a>
              </div>

              <style>

                {% comment %} style for first image  {% endcomment %}
                #block-{{block.id}} .custom-secondry-image{
                  width: {{block.settings.secondary_image_width}}px;
                }
                {% comment %} for rotate and flip  {% endcomment %}
                {% if block.settings.first_image_flip == 'horizontal' %}
                  #block-{{block.id}} .custom-secondry-image img{
                    -webkit-transform: scaleX(-1) rotate(calc({{block.settings.secondary_image_rotate}} * 3.6deg));
                    transform: scaleX(-1) rotate(calc({{block.settings.secondary_image_rotate}} * 3.6deg));
                  }
                {% elsif block.settings.first_image_flip == 'verticle' %}
                  #block-{{block.id}} .custom-secondry-image img{
                    -webkit-transform: scaleY(-1) rotate(calc({{block.settings.secondary_image_rotate}} * 3.6deg));
                    transform: scaleY(-1) rotate(calc({{block.settings.secondary_image_rotate}} * 3.6deg));
                  }
                {% else %}
                  #block-{{block.id}} .custom-secondry-image img{
                    transform: rotate(calc({{block.settings.secondary_image_rotate}} * 3.6deg));
                  }
                {% endif %}
                {% comment %} end rotate and flip  {% endcomment %}
                {% if block.settings.first_image_vertical_alignment == 'top' %}
                  #block-{{block.id}} .custom-secondry-image{
                    top: {{block.settings.secondary_image_top}}%;
                  }
                {% elsif block.settings.first_image_vertical_alignment == 'bottom' %}
                  #block-{{block.id}} .custom-secondry-image{
                    bottom: {{block.settings.secondary_image_bottom}}%;
                  }
                {% elsif block.settings.first_image_vertical_alignment == 'middle' and block.settings.first_image_horizontal_alignment != 'center' %}
                  #block-{{block.id}} .custom-secondry-image{
                    top: 50%;
                    transform: translate(0% , -50%);
