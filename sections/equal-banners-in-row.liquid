<script>
  
(function() {
    const $slider = $('#section-{{section.id}} .image-slider-for-3');
    
    function initSlider() {
        if ($(window).width() <= 1150) {
            if (!$slider.hasClass('slick-initialized')) {
                $slider.slick({
                    slidesToShow: 2.4,
                    slidesToScroll: 1,
                    arrows: false,
                    infinite: false,
                    responsive: [
                      {
                        breakpoint: 1050,
                        settings: {
                            slidesToShow: 2.1,
                            slidesToScroll: 1,
                        }
                        },
                        {
                        breakpoint: 990,
                        settings: {
                            slidesToShow: 2,
                            slidesToScroll: 1,
                        }
                        },
                        {
                          breakpoint: 770,
                          settings: {
                              slidesToShow: 1.8,
                              slidesToScroll: 1,
                          }
                        },
                        {
                        breakpoint: 690,
                        settings: {
                            slidesToShow: 1.6,
                            slidesToScroll: 1,
                        }
                        },
                        {
                        breakpoint: 525,
                        settings: {
                            slidesToShow: 1.15,
                            slidesToScroll: 1,
                        }
                        }
                    ]
                });
            }
        } else {
            if ($slider.hasClass('slick-initialized')) {
                $slider.slick('unslick');
            }
        }
    }

    initSlider();

    
    $(window).on('resize', function() {
        initSlider();
    });
})();


