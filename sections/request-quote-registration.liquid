<section class="req-qt-all" data-section-id="{{ section.id }}" data-section-type="register">
  <!--Contener Page Wrap Start-->
  <div class="container">
    <div class="log-temp custom-log-temp custon-quate">
      <!--Left Panel Start -->
      <div class="log-temp-cell custom-log-temp-cell with-custom-bradcrumb reql" style="margin:0px !important;">
        <div class="">
          <div class="snak-bar">
            <span>
              <img src="https://cdn.shopify.com/s/files/1/2372/2681/files/atention-icon-1.png?v=1678445130" alt="" loading="lazy"/>
            </span>
            In order to submit your quote request you will need to have an account.
          </div>
          <div class="quate-new-form" id="quote-request-registration-section">
            <p>Have an account ? <span id="show-quote-request-login-section" style="color: #06a0e3 !important;text-decoration: underline;cursor: pointer;">Sign in</span> to submit your request</p>

            <div class="cf-form">
              <h2 class="custom-inr-page-heading">Create Your Account</h2>
              <form data-cf-form="8bt32D"></form>
            </div>
          </div>
          <div class="quate-new-form" id="quote-request-login-section" style="display: none;">
            <p>New to Monitors.com? <span id="show-quote-request-registration-section" style="color: #06a0e3 !important;text-decoration: underline;cursor: pointer;">Create an account</span> to request a quote</p>

            <div style="animation: cf-fadein 500ms cubic-bezier(0.11, 0.33, 0.24, 1);">
              {%- comment -%}
              --------------------------------------------------------------------------------------------------------------------
              CUSTOMER LOGIN FORM
              --------------------------------------------------------------------------------------------------------------------
              {%- endcomment -%}

              {%- if is_recover_active -%}
                {%- assign login_default_visibility = 'display: none' -%}
              {%- else -%}
                {%- assign login_default_visibility = 'display: block' -%}
              {%- endif -%}

              {%- form 'customer_login', name: 'login', class: 'form form--main', id: 'customer_login', style: login_default_visibility -%}
                <h2 class="custom-inr-page-heading">Sign in</h2>
                <p class="custom-small">Enter your email and password:</p>
                {%- if form.errors -%}
                  <p class="alert alert--error">{{ form.errors.messages['form'] }}</p>
                {%- endif -%}

                <div class="form__input-wrapper ">
                  <input type="email" id="customer[email]" name="customer[email]" class="form__field form__field--text" placeholder="Email" required="">
                </div>

                <div class="form__input-wrapper ">
                  <input type="password" id="customer[password]" name="customer[password]" class="form__field form__field--text" placeholder="Password" required="">
                </div>

                <input type="hidden" name="checkout_url" value="{{ pages['new-addresses'].url }}">

                <div class="qt-botton-row">
                  <div class="qt-botton-row-lost">
                    <p class="desk-hide forget-custom">
                      <span>Lost password?</span>
                      <button type="button" data-action="toggle-login-form" class="link link--accented" id="show_recover_customer_password"> Recover Password</button>
                    </p>
                  </div>
                  <div>
                    <button type="submit" class="button button--primary button--full">Login</button>
                  </div>
                </div>
              {%- endform -%}


              {%- comment -%}
              --------------------------------------------------------------------------------------------------------------------
              CUSTOMER RECOVERY FORM
              --------------------------------------------------------------------------------------------------------------------
              {%- endcomment -%}

              {%- if is_recover_active -%}
                {%- assign recover_default_visibility = 'display: block' -%}
              {%- else -%}
                {%- assign recover_default_visibility = 'display: none' -%}
              {%- endif -%}

              {%- form 'recover_customer_password', name: 'recover', class: 'form form--main', id: 'recover_customer_password', style: recover_default_visibility -%}
