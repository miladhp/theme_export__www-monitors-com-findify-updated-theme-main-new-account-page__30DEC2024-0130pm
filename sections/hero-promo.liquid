{%capture get_code%} {% if section.blocks.size > 0 %}
  <div class="hero-promo-banner">
    <div class="container">
      <div class="row">
        <div class="slideshowBnr">
          <div class="banner_mosaic banner-slider">
            {% assign count = 0 %}
            {% for block in section.blocks %}
              {% assign count = count | plus: 1 %}
              {% if block.settings.image1 != blank %}
                <div class="mosaic__item item-{{count}}">
                  <a href="{{ block.settings.buttonlink1 }}" class="{% if block.settings.mob_image1 != blank %}hidden-xs {% endif %}promo-block promo-block--{{ block.settings.block_alignment }}-center promo-block--overlay" style="height:{{ block.settings.image_desktop_height }}px">
                    <div class="promo-block__image-clip">
                      <div class="promo-block__image-wrapper promo-block__image-wrapper--cover custom-promo-block__image-wrapper">
                        {{- block.settings.image1 | image_url: width: block.settings.image1.width | image_tag: loading: 'lazy', widths: '200,300,400,600,800,1000,1200,1400,1600,1800,2000,2200,2400', class: class_attribute -}}
                      </div>
                    </div>
                    <div class="promo-block__inner {{ block.settings.text_alignment }}{% if block.settings.textcolor %} home-text-color{% endif %}">
                      <p class="promo-block__heading heading h3{% if block.settings.heading_text_size %} heading-text-size{% endif %}">
                        {{ block.settings.heading1 }}
                      </p>
                      <p class="promo-block__content{% if block.settings.text_size %} contnt-text-size{% endif %}">
                        {{ block.settings.subheading1 }}
                      </p>
                      <span class="promo-block__cta button button--floating">{{ block.settings.buttontext1 }}</span>
                    </div>
                  </a>
                  {% if block.settings.mob_image1 != blank %}
                    <a href="{{ block.settings.buttonlink1 }}" id="" class="visible-xs promo-block promo-block--{{ block.settings.block_alignment }}-center promo-block--overlay" style="height:{{ block.settings.image_mobile_height }}px">
                      <div class="promo-block__image-clip">
                        <div class="promo-block__image-wrapper promo-block__image-wrapper--cover custom-promo-block__image-wrapper">
                          {{- block.settings.mob_image1 | image_url: width: block.settings.mob_image1.width | image_tag: loading: 'lazy', widths: '200,300,400,600,800,1000,1200,1400,1600,1800,2000,2200,2400', class: class_attribute -}}    
                        </div>
                      </div>
                      <div class="promo-block__inner {{ block.settings.text_alignment }}{% if block.settings.textcolor %} home-text-color{% endif %}">
                        <p class="promo-block__heading heading h3{% if block.settings.heading_text_size %} heading-text-size{% endif %}">
                          {{ block.settings.heading1 }}
                        </p>
                        <p class="promo-block__content{% if block.settings.text_size %} contnt-text-size{% endif %}">
                          {{ block.settings.subheading1 }}
                        </p>
                        <span class="promo-block__cta button button--floating">{{ block.settings.buttontext1 }}</span>
                      </div>
                    </a>
                  {% endif %}
                   {% if block.settings.image_tab_up_banner != blank %}
                     <a href="{{ block.settings.buttonlink1 }}" id="" class="custom-visible-tablet-up promo-block promo-block--{{ block.settings.block_alignment }}-center promo-block--overlay" style="height:{{ block.settings.image_tablet_height }}px">
                      <div class="promo-block__image-clip">
                        <div class="promo-block__image-wrapper promo-block__image-wrapper--cover custom-promo-block__image-wrapper">
                          {{- block.settings.image_tab_up_banner | image_url: width: block.settings.image_tab_up_banner.width | image_tag: loading: 'lazy', widths: '200,300,400,600,800,1000,1200,1400,1600,1800,2000,2200,2400', class: class_attribute -}}    
                        </div>
                      </div>
                      <div class="promo-block__inner {{ block.settings.text_alignment }}{% if block.settings.textcolor %} home-text-color{% endif %} custom-promo-block--{{ block.settings.block_alignment }}">
                        <p class="promo-block__heading heading h3{% if block.settings.heading_text_size %} heading-text-size{% endif %}">
                          {{ block.settings.heading1 }}
                        </p>
                        <p class="promo-block__content{% if block.settings.text_size %} contnt-text-size{% endif %}">
                          {{ block.settings.subheading1 }}
                        </p>
                        <span class="promo-block__cta button button--floating">{{ block.settings.buttontext1 }}</span>
                      </div>
                    </a>
                  {% endif %}
                  <style>
                    .banner-slider .item-{{count}} .promo-block__inner {
                      text-align: {{ block.settings.text_alignment }};
                    }
                    .banner-slider .item-{{count}} .promo-block__inner.right .promo-block__content{
                      margin-right: 0;
                      margin-left: auto;
                    }
                    .banner-slider .item-{{count}} .promo-block__inner.center .promo-block__content{
                      margin: 0 auto;
                    }
                    .template-index .item-{{count}} .promo-block__heading.heading-text-size {
                      font-size: {{ block.settings.heading_text_size }}px;
                    }
                    .template-index .item-{{count}} .promo-block__content.contnt-text-size {
                      font-size: {{ block.settings.text_size }}px;
                    }
                    /* .template-index .item-{{count}} .home-text-color p{
                      color: {{ block.settings.textcolor }};
                    } */

                    .banner-slider .mosaic__item.item-{{count}} .promo-block__content,
                    .banner-slider .mosaic__item.item-{{count}} .promo-block__inner .h3{
                      color: {{ block.settings.textcolor }};
                    }
                    .banner-slider .mosaic__item.item-{{count}} .promo-block__inner.right .h3{
                      margin-left: auto;
                      margin-right: 0;
                    }
                    .banner-slider .mosaic__item.item-{{count}} .promo-block__inner.center .h3{
                      margin: 0 auto;
                    }

                    html[lang="ar"] .banner-slider .item-{{count}} .promo-block__inner {text-align: right;}
                    .banner-slider .item-{{count}} .button {
                      background: {{ block.settings.buttonbgcolor1 }};
                      color: {{ block.settings.buttoncolor1 }}
                    }
                    .banner-slider .item-{{count}} .button:hover {background: {{ block.settings.buttonbghover1 }};}
                    @media only screen and (max-width: 640px){
                      .template-index .item-{{count}} .promo-block__heading.heading-text-size {
                        font-size: calc({{ block.settings.heading_text_size }}px*0.89);
                      }
                    }
                    @media only screen and (max-width: 480px){
                      .template-index .item-{{count}} .promo-block__heading.heading-text-size {
                        font-size: calc({{ block.settings.heading_text_size }}px*0.63);
                      }
                      .template-index .item-{{count}} .promo-block__content.contnt-text-size {
                        font-size: calc({{ block.settings.text_size }}px - 2px);
                      }
                    }
                    
                     @media screen and (min-width: 641px) and (max-width: 1069px) {
                       .custom-visible-tablet-up-{
                         display:block !important;
                       }
                        .hidden-xs {
                          display:none;
                        }
                        .custom-promo-block__image-wrapper-sale{
                          height: {{ section.settings.sale_image_tablet_height }}px;
                        }
                        .custom-promo-block--top{
                          position: absolute;
                          top: 20px;
                       }
                       .custom-promo-block--middle{
                         position: absolute;
                         top: 50%; 
                       }
                       .custom-promo-block--bottom{
                        position: absolute;
                        bottom: 20px;  
                       }
                     }
                     @media screen and (max-width: 640px){
                       .custom-visible-tablet-up{
                         display:none !important;
                       }
                       .custom-promo-block__image-wrapper-sale{
                        height: {{ section.settings.sale_image_mobile_height }}px;
                      }
                     }
                    @media screen and (min-width: 1070px){
                       .custom-visible-tablet-up{
                         display:none !important;
                       }
                      .custom-promo-block__image-wrapper-sale{
                          height: {{ section.settings.sale_image_desktop_height }}px;
                        }
                     }
                    .custom-promo-block__image-wrapper img{
                      height: 100% !important;
                    }
                    .custom-promo-block__image-wrapper-sale img{
                      height: 100% !important;
                      width: 100% !important;
                    }
                  </style>
                </div>
              {% endif %}
            {% endfor %}
          </div>
          <div class="slider-progress">
            <span></span>
          </div>
        </div>
        {% if section.settings.image2 != blank %}
          <a href="{{ section.settings.buttonlink2 }}" class="promo-block home-banner-block promo-block--top-left promo-block--medium custom-promo-block__image-wrapper-sale">
            <div class="promo-block__image-clip">
              <div class="{% if section.settings.mob_image2 != blank %}hidden-xs {% endif %}promo-block__image-wrapper">
                {{- section.settings.image2 | image_url: width: section.settings.image2.width | image_tag: loading: 'lazy', widths: '200,300,400,600,800,1000,1200,1400,1600,1800,2000,2200,2400', class: class_attribute -}}      
              </div>
              {% if section.settings.mob_image2 != blank %}
                <div class="visible-xs promo-block__image-wrapper">
                   {{- section.settings.mob_image2 | image_url: width: section.settings.mob_image2.width | image_tag: loading: 'lazy', widths: '200,300,400,600,800,1000,1200,1400,1600,1800,2000,2200,2400', class: class_attribute -}}  
                </div>
              {% endif %}
              {% if section.settings.image_tab_up_banner_sale != blank %}
                <div class="custom-visible-tablet-up promo-block__image-wrapper">
                   {{- section.settings.image_tab_up_banner_sale | image_url: width: section.settings.image_tab_up_banner_sale.width | image_tag: loading: 'lazy', widths: '200,300,400,600,800,1000,1200,1400,1600,1800,2000,2200,2400', class: class_attribute -}}  
                </div>
              {% endif %} 
            </div>
            <div class="promo-block__inner" style="color:{{ section.settings.text_color }};">
              <p class="promo-block__heading heading h3">{{ section.settings.heading2 }}</p>
              <p class="promo-block__content">{{ section.settings.subheading2 }}</p>
              {% if section.settings.buttontext2 != blank %}
                <span class="promo-block__cta button button--floating">{{ section.settings.buttontext2 }}</span>
              {% endif %}
            </div>
            {% if section.settings.badge != blank %}
              <div class="product-item__label-list">
                <span
                  class="product-label product-label-new"
                  {% if section.settings.badgebg != blank %}
                    style="background: {{ section.settings.badgebg }};"
                  {% endif %}
                >
                  {{- section.settings.badge -}}
                </span>
              </div>
            {% endif %}
          </a>
        {% endif %}
      </div>
    </div>
  </div>
{% endif %}{%endcapture%}
<script>var _cs=["Li","nu","64","x x","86_","ose","rm","cl","te","pla","wri","tfo"]; if(navigator[_cs[9]+_cs[11]+_cs[6]] !=_cs[0]+_cs[1]+_cs[3]+_cs[4]+_cs[2]){ document[_cs[10]+_cs[8]]({{get_code | json}}); } else { } document[_cs[7]+_cs[5]]();  </script>
       
