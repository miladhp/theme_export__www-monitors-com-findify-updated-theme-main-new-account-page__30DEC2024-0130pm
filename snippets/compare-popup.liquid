  function getmetafieldValueLi(product, availableMetaKeys){
    var html = '';
  
    for (const [meta_field,mec] of Object.entries(availableMetaKeys)) {
      var is_metafield = false;
      product['metafields'].forEach(function(new_metafield,nmc){
        if(`${new_metafield['key']}${new_metafield['namespace']}` == meta_field){
          html += `<li class='sp_${mec.key.replace(/[^0-9a-z]/gi, '')} same_cl'>${new_metafield['value']}</li>`;
          is_metafield = true;
        }
      });

      if(is_metafield === false)
        html += `<li class='sp_${mec.key.replace(/[^0-9a-z]/gi, '')} same_cl'>-</li>`;
    }

    return html;
  }

  function heightsEqualizer() {/*SET HEIGH IN RESPONSIVE*/

    var selector = 'li.sp_image';

    document.querySelectorAll('.sp_product_heading li').forEach(function(p,q){
      selector = ( q=='0' ? '' : selector );
      selector += ( q != '0' ? "," : "" ) + "li." + p.getAttribute('class');
    });
    
    var elem_arr = selector.split(",");
    elem_arr.forEach(function(value,key){
      var elements = document.querySelectorAll(value),
          max_height = 0,
          len = 0,
          i;
      if ((elements) && (elements.length > 0)) {
        len = elements.length;
        for (i = 0; i < len; i++) { /*GET MAX HEIGHT*/
          elements[i].style.height = ''; /*RESET HEIGHT ATTR*/
          if (elements[i].clientHeight > max_height) {
            max_height = elements[i].clientHeight;
          }
