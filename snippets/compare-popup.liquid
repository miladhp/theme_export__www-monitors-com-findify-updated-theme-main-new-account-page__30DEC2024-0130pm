            var compareMinPrice = '';
            
            var productImage = '';
            if(!product.variant.is_default || product.all_images[1] == undefined){
              productImage = product.variant.image;
              proMinPrice = product.minVariantPrice;
              compareMinPrice = product.minComparePrice;
            }
            else{
              productImage = product.all_images[1].src
              proMinPrice = product.minVariantPrice;
              compareMinPrice = product.minComparePrice;
            }
            
              productDataUl += `<ul class='sp_product_con fill_cmp'>` +
                `<li class='sp_image'>` +
                `<div class="sp_image_block">` +
                `<a spcmp_vid="${product.variant.id}" spcmp_pid="${product.id}" href="javascript:void(0);" class="sp_close">✕</a>` +
                `<a target="_blank" href='/products/${product.handle}?variant=${product.variant.id}' class="sp_img">` +
                `<img src="${productImage}" alt="">` +
                `</a>` +
                `</div>`+
                `<div class="sp_title">${product.title}</div>` +
                `<div class="sp_var_title">`+/*${product.variant.title}*/`</div>`;
            
            productDataUl += `<a class="product-item__reviews-badge link" href="/products/${product.handle}?variant=${product.variant.id}#review" style="height:25px">
                <span class=" stamped-product-reviews-badge" data-product-sku="${product.handle}" data-id="${product.id}" data-product-type="${product.ptype}" data-product-title='${product.title}'  style="display:block;"></span>
              </a>`;
                
              if(product.variant.compare_at_price > product.variant.price){
                var savings = "<span>".concat(Currency.formatMoney(product.variant.compare_at_price - product.variant.price, currencyFormat).replace('.00',''), "</span>");
                productDataUl += `<span class="product-label product-label--on-sale">Save <span>${savings}</span></span>` +
                 `<div class="sp_price">`;
                  if(proMinPrice != ''){
                    productDataUl += `<span class="money sale-price">From</span>`;
                    productDataUl += `<span class="money sale-price" data-wg-notranslate="manual">${Currency.formatMoney(proMinPrice, currencyFormat)}</span>`;
                  }else{
                    productDataUl += `<span class="money sale-price" data-wg-notranslate="manual">${Currency.formatMoney(product.variant.price, currencyFormat)}</span>`; 
                  }
                  if(compareMinPrice != '' && compareMinPrice > proMinPrice)
                    productDataUl += `<strike class="money">${Currency.formatMoney(compareMinPrice, currencyFormat)}</strike>`;
                  else
