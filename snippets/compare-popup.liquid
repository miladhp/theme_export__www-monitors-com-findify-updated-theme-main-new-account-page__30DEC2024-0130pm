             productDataUl += `<div class="sp_add_to_cart"><a class='sa_cmp_addcart' href="/products/${product.handle}?variant=${product.variant.id}">View Product</a></div>`;
            
//             if(product.variant.avail_or_sold == '1'){
//               productDataUl += `<div class="sp_add_to_cart"><form action='/cart/add' method='post' enctype='multipart/form-data'><input type='hidden' name='id' value='${product.variant.id}'><input type='hidden' name='quantity' value='1'><input class='sa_cmp_addcart' name='button' value='Add to Cart' type='submit'></form></div>`;
