    { code: "USD", symbol: "$" },
    { code: "CAD", symbol: "$" },
    { code: "EUR", symbol: "€" },
    { code: "GBP", symbol: "£" },
    { code: "ZAR", symbol: "R" },
    { code: "VND", symbol: "₫" },
    { code: "UAH", symbol: "₴" },
    { code: "TRY", symbol: "₺" },
    { code: "THB", symbol: "฿" },
    { code: "SGD", symbol: "$" },
    { code: "SEK", symbol: "kr" },
    { code: "SAR", symbol: "ر.س" },
    { code: "RON", symbol: "Lei" },
    { code: "QAR", symbol: "ر.ق" },
    { code: "PLN", symbol: "zł" },
    { code: "NZD", symbol: "$" },
    { code: "NOK", symbol: "kr" },
    { code: "MYR", symbol: "RM" },
    { code: "MKD", symbol: "ден" },
    { code: "KRW", symbol: "₩" },
    { code: "JPY", symbol: "¥" },
    { code: "ISK", symbol: "kr" },
    { code: "INR", symbol: "₹" },
    { code: "HUF", symbol: "Ft" },
    { code: "HKD", symbol: "$" },
    { code: "DKK", symbol: "kr." },
    { code: "CZK", symbol: "Kč" },
    { code: "CNY", symbol: "¥" },
    { code: "CHF", symbol: "CHF" },
    { code: "BGN", symbol: "лв." },
    { code: "AUD", symbol: "$" },
    { code: "AED", symbol: "د.إ" }
  ];
  
  function getCurrencySymbol(currencyCode) {
    const currency = currencyList.find(c => c.code === currencyCode);
    const symbol = currency ? currency.symbol : "$";
    return symbol;
  }
  
    quotes.forEach(quote => {
