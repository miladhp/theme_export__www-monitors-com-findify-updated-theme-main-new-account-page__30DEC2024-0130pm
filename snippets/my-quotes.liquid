     function setupPagination(currentPage, lastPage, hasNext, hasPrev) {
    const paginationContainer = document.getElementById('pagination-container');
    paginationContainer.innerHTML = '';

    const paginationList = document.createElement('ul');
    paginationList.classList.add('pagination-list');
    
    const prevButton = document.createElement('li');
    prevButton.classList.add('pagination-item');
    prevButton.textContent = 'Prev';
    prevButton.classList.toggle('disabled', !hasPrev);
    if (hasPrev) {
        prevButton.addEventListener('click', () => changePage(currentPage - 1));
    }
    paginationList.appendChild(prevButton);
    
    for (let i = 1; i <= lastPage; i++) {
        if (i === currentPage || (i <= 3) || (i === lastPage) || (i >= currentPage - 1 && i <= currentPage + 1)) {
            const pageItem = document.createElement('li');
            pageItem.classList.add('pagination-item');
            pageItem.textContent = i;
            if (i === currentPage) {
                pageItem.classList.add('active');
            } else {
                pageItem.addEventListener('click', () => changePage(i));
            }
            paginationList.appendChild(pageItem);
        } else if (i === 4 || (i === currentPage + 2 && i < lastPage - 1)) {
            const dots = document.createElement('li');
            dots.classList.add('pagination-item');
            dots.textContent = '...';
            paginationList.appendChild(dots);
        }
    }

    const nextButton = document.createElement('li');
    nextButton.classList.add('pagination-item');
    nextButton.textContent = 'Next';
    nextButton.classList.toggle('disabled', !hasNext);
    if (hasNext) {
        nextButton.addEventListener('click', () => changePage(currentPage + 1));
    }
    paginationList.appendChild(nextButton);

    paginationContainer.appendChild(paginationList);
  }

  function changePage(page) {
    currentPage = page;
    fetchQuotes(currentPage);
  }
  
