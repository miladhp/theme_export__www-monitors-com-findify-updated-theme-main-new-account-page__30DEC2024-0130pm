<div id="quote-container"></div>
<div id="pagination-container" class="pagination-wrapper"></div>

<script>

  const emptyQuotesContainer = `
    <div class="no-quotes-container">
                <div class="no-quotes">
                    {% render 'empty-order-box-icon.liquid' %}
                    <h3>
                        You haven't requested for any quotes yet.
                    </h3>
                </div>
        </div>
  `;

  let currentPage = 1;
  const perPage = 10;
  const customerId = {{ customer.id }};

  function fetchQuotes(page) {
    fetch(`https://developmentquote.monitors.com/shopify/customer/quotes/v1?customer_id=${customerId}&page=${page}&per_page=${perPage}`)
    .then(response => response.json())
    .then(data => {
        displayQuotes(data.records);
        setupPagination(data.current_page, data.last_page, data.has_next_page, data.has_prev_page);
    })
    .catch(error => console.error('Error fetching quote data:', error));
  }
  
function displayQuotes(quotes) {
    const container = document.getElementById('quote-container');
    container.innerHTML = ''; 

    if (!quotes || quotes.length === 0) {
        container.innerHTML = emptyQuotesContainer;
        return;
    }

