// function to handle download of PDF 

function handleDownloadPDF(quoteNumber){
   fetch(`https://developmentquote.monitors.com/shopify/customer/invoice?quote_number=${quoteNumber}`)

   .then(response => {
     if (response.ok) {
       return response.json();
     } else {
       throw new Error('Failed to fetch invoice data');
     }
   })
   .then(data => {
    console.log("data", data)
     if (data && data.data) {
        fetch(data.data)
          .then(pdfResponse => {
            if (pdfResponse.ok) {
              return pdfResponse.blob();
            } else {
              throw new Error('Failed to fetch the PDF file');
            }
          })
          .then(blob => {
            const downloadUrl = URL.createObjectURL(blob);
            
            const link = document.createElement('a');
            link.href = downloadUrl;
            link.download = `invoice_${quoteNumber}.pdf`;
            link.click();
            
            URL.revokeObjectURL(downloadUrl);
          })
          .catch(error => {
            console.error('Error fetching PDF:', error);
            alert('Failed to retrieve the PDF. Please try again.');
          });
     } else {
       alert('No URL found in the response');
     }
   })
   .catch(error => {
     console.error('Error:', error);
     alert('Failed to retrieve the PDF. Please try again.');
   });
}

// end download function

// handle refresh a quote

function handleRefreshQuote(e, quoteId,button){
  e.preventDefault();

  const quotes = document.querySelector('#my-quotes');
  let customerId;

  if(quotes){
    customerId = quotes.getAttribute('data-customer-id');
  }
  
  //console.log("Refreshed...!!!", quoteId, customerId);

  const formData = new FormData();
  formData.append('customer_id', customerId);
  formData.append('quote_id', quoteId);

  fetch('https://developmentquote.monitors.com/shopify/customer/refresh-quote', {
    method: 'POST',
    body: formData
  })
  .then(response => response.json())
  .then(data => {
    if (data.status) {

      const parentElement = button.parentElement;
      button.remove();
      parentElement.style.justifyContent = "end";
      const successMessage = document.createElement('span');
      successMessage.style.color = "#DF8600";
      successMessage.textContent = 'Quote Refresh Pending';
      parentElement.appendChild(successMessage);

      const mainStatus = parentElement.closest('.quote-item').querySelector('.quote-item-status-value.custom-q-value');

      mainStatus.innerHTML = "Quote Refresh Pending";
      mainStatus.style.color = "#DF8600";
      
    } else {
      console.log('Failed to refresh the quote:', data.message);
    }
  })
  .catch(error => {
    console.error('Error refreshing quote:', error);
  });



}


