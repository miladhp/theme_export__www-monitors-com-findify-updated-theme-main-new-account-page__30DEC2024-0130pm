                <div class="cat-item-img">
                  {% comment %} <img class="" src="{{block.settings.m_cat_mob_icon2 | img_url: '' }}" alt=""> {% endcomment %}
                  {{- block.settings.m_cat_mob_icon2 | image_url: width: block.settings.m_cat_mob_icon2.width | image_tag: loading: 'lazy', sizes: 'min(100vw, 520px)', widths: '300,400,500,600' -}}
                </div>
                <label for="">{{block.settings.m_cat_label2}}</label>
              </a>
            </li>
            {% endif %}
            {% if block.settings.m_cat_label3 != blank %}
            <li class="cat-item">
              <a href="{{ '/collections/clinical-review-displays' }}" class="df fww">
                <div class="cat-item-img">
                  {% comment %} <img class="" src="{{block.settings.m_cat_mob_icon3 | img_url: '' }}" alt=""> {% endcomment %}
