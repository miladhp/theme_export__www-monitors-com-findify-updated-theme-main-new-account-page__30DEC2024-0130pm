              {% if block.settings.m_size_item_label8 !=blank and block.settings.m_size_item_url8 != blank %}
              <li class="{% if block.settings.m_size_show_ribbons8 %}riban{% endif %}"><a href="{{block.settings.m_size_item_url8}}">{{block.settings.m_size_item_label8}}</a></li>
