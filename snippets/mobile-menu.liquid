            {% endif %}
            {% if block.settings.m_cat_label6 != blank %}
            <li class="cat-item">
              <a href="{{ '/collections/pathology-displays' }}" class="df fww">
                <div class="cat-item-img">
                  {% comment %} <img class="" src="{{block.settings.m_cat_mob_icon3 | img_url: '' }}" alt=""> {% endcomment %}
