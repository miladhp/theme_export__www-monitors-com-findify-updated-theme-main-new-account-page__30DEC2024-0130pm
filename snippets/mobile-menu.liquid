                </div>
                <label for="">{{ block.settings.m_cat_label6 | replace: '<br>', '' }}</label>
              </a>
            </li>
            {% endif %}
            {% if block.settings.m_cat_label4 != blank %}
            <li class="cat-item">
              <a href="{{ '/collections/surgical-displays' }}" class="df fww">
                <div class="cat-item-img">
                  {% comment %} <img class="" src="{{block.settings.m_cat_mob_icon4 | img_url: '' }}" alt=""> {% endcomment %}
                  {{- block.settings.m_cat_mob_icon4 | image_url: width: block.settings.m_cat_mob_icon4.width | image_tag: loading: 'lazy', sizes: 'min(100vw, 520px)', widths: '300,400,500,600' -}}
                </div>
                <label for="">{{ block.settings.m_cat_label4 | replace: '<br>', '' }}</label>
              </a>
            </li>
            {% endif %}
            {% if block.settings.m_cat_label5 != blank %}
            <li class="cat-item">
              <a href="{{ '/collections/professional-displays' }}" class="df fww">
                <div class="cat-item-img">
                  {% comment %} <img class="" src="{{block.settings.m_cat_mob_icon5 | img_url: '' }}" alt=""> {% endcomment %}
                 {{- block.settings.m_cat_mob_icon5 | image_url: width: block.settings.m_cat_mob_icon5.width | image_tag: loading: 'lazy', sizes: 'min(100vw, 520px)', widths: '300,400,500,600' -}}
