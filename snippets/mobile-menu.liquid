{%- assign mega_menus = section.blocks | map: 'settings' | map: 'menu_item' -%}

<div id="mobile-menu" class="mobile-menu custom-mobile-menu" aria-hidden="true">
  {% comment %}
  {%- render 'icon', icon: 'nav-triangle-borderless' -%}
  {% endcomment %}

  <div class="mobile-menu__inner">
    <div class="mobile-menu__panel">
      <div class="mobile-menu__section">
        <ul class="mobile-menu__nav" data-type="menu">
          <li class="mobile-menu__nav-item lopa"><a href="/collections/monitors" class="mobile-menu__nav-link" data-type="menuitem">Products</a></li>
          {% for block in section.blocks %}
          {% if block.type == 'monitors_menu' %}
          <li class="mobile-menu__nav-item lopa">
            <button class="mobile-menu__nav-link" data-type="menuitem" aria-haspopup="true" aria-expanded="false" aria-controls="mobile-panel-11" data-action="open-panel">
              <div class="menu-icon"><img src="{{block.settings.m_mob_menu_icon | img_url: '' }}" alt="" loading="lazy"></div> <span>{{block.settings.title}}</span> {%- render 'icon', icon: 'arrow-right' -%}
            </button>
          </li>
          {% endif %}
          {% endfor %}
          {% for block in section.blocks %}
          {% if block.type == 'workstations_menu' %}
          <li class="mobile-menu__nav-item lopa">
            <button class="mobile-menu__nav-link" data-type="menuitem" aria-haspopup="true" aria-expanded="false" aria-controls="mobile-panel-12" data-action="open-panel">
              <div class="menu-icon"><img src="{{block.settings.w_mob_menu_icon | img_url: '' }}" alt="" loading="lazy"></div> <span>{{block.settings.title}}</span> {%- render 'icon', icon: 'arrow-right' -%}
            </button>
          </li>
          {% endif %}
          {% endfor %}
          {% for block in section.blocks %}
          {% if block.type == 'crs_menu' %}
          <li class="mobile-menu__nav-item lopa">
            <button class="mobile-menu__nav-link" data-type="menuitem" aria-haspopup="true" aria-expanded="false" aria-controls="mobile-panel-13" data-action="open-panel">
              <div class="menu-icon"><img src="{{block.settings.crs_mob_menu_icon | img_url: '' }}" alt="" loading="lazy"></div> <span>{{block.settings.title}}</span> {%- render 'icon', icon: 'arrow-right' -%}
            </button>
          </li>
          {% endif %}
          {% endfor %}
          {% for block in section.blocks %}
          {% if block.type == 'graphic_menu' %}
          <li class="mobile-menu__nav-item lopa">
            <button class="mobile-menu__nav-link" data-type="menuitem" aria-haspopup="true" aria-expanded="false" aria-controls="mobile-panel-15" data-action="open-panel">
              <div class="menu-icon"><img src="{{block.settings.gc_mob_menu_icon | img_url: '' }}" alt="" loading="lazy"></div> <span>{{block.settings.title}}</span> {%- render 'icon', icon: 'arrow-right' -%}
            </button>
          </li>
          {% endif %}
          {% endfor %}
          {% for block in section.blocks %}
          {% if block.type == 'dictation_menu' %}
          <li class="mobile-menu__nav-item lopa">
            <button class="mobile-menu__nav-link" data-type="menuitem" aria-haspopup="true" aria-expanded="false" aria-controls="mobile-panel-16" data-action="open-panel">
              <div class="menu-icon"><img src="{{block.settings.dm_mob_menu_icon | img_url: '' }}" alt="" loading="lazy"></div> <span>{{block.settings.title}}</span> {%- render 'icon', icon: 'arrow-right' -%}
            </button>
          </li>
          {% endif %}
          {% endfor %}
          {% for block in section.blocks %}
          {% if block.type == 'accessories_menu' %}
          <li class="mobile-menu__nav-item lopa">
            <button class="mobile-menu__nav-link" data-type="menuitem" aria-haspopup="true" aria-expanded="false" aria-controls="mobile-panel-17" data-action="open-panel">
              <div class="menu-icon"><img src="{{block.settings.ac_mob_menu_icon | img_url: '' }}" alt="" loading="lazy"></div> <span>{{block.settings.title}}</span> {%- render 'icon', icon: 'arrow-right' -%}
            </button>
          </li>
          {% endif %}
          {% endfor %}
          {% for block in section.blocks %}
          {% if block.type == 'film_menu' %}
          <li class="mobile-menu__nav-item lopa">
            <button class="mobile-menu__nav-link" data-type="menuitem" aria-haspopup="true" aria-expanded="false" aria-controls="mobile-panel-14" data-action="open-panel">
              <div class="menu-icon"><img src="{{block.settings.fd_mob_menu_icon | img_url: '' }}" alt="" loading="lazy"></div> <span>{{block.settings.title}}</span> {%- render 'icon', icon: 'arrow-right' -%}
            </button>
          </li>
          {% endif %}
          {% endfor %}

          {%- for link in menu.links -%}
            <li class="mobile-menu__nav-item navigation-menu-dynamic">
              {%- if link.links != blank -%}
                {%- capture panel_id -%}mobile-panel-{{ forloop.index0 }}{%- endcapture -%}
                <button class="mobile-menu__nav-link" data-type="menuitem" aria-haspopup="true" aria-expanded="false" aria-controls="{{ panel_id }}" data-action="open-panel">
                  {{- link.title -}} {%- render 'icon', icon: 'arrow-right' -%}
                </button>
              {%- else -%}
                <a href="{{ link.url }}" class="mobile-menu__nav-link" data-type="menuitem">{{ link.title }}</a>
              {%- endif -%}
            </li>
          {%- endfor -%}
        </ul>
      </div>
      <div class="mobcurr">
        <ul class="mobile-menu__nav" data-type="menu">
          <li class="mobile-menu__nav-item">
            <button class="mobile-menu__nav-link custom-mobile-currency-selector" data-type="menuitem" aria-haspopup="true" aria-expanded="false" aria-controls="mobile-panel-1" data-action="open-panel"> 
              {%- form 'localization', id: 'localization_form_header_currency_mobile_selected' -%}{{ form.current_currency.iso_code }} {% if form.current_currency.symbol %}{{ form.current_currency.symbol }}{% endif %}{%- endform -%} {%- render 'icon', icon: 'arrow-right' -%}
            </button>
          </li>
          <li class="mobile-menu__nav-item">
            <button class="mobile-menu__nav-link custom-mobile-language-selector" data-type="menuitem" aria-haspopup="true" aria-expanded="false" aria-controls="mobile-panel-2" data-action="open-panel">
              {%- form 'localization', id: 'localization_form_header_locale_mobile_selected' -%}{{ form.current_locale.endonym_name | capitalize }}{%- endform -%} {%- render 'icon', icon: 'arrow-right' -%}
            </button>
          </li> 
        </ul>
      </div>
    </div>

    {%- comment -%}We now need to loop again (for the second level) to create the nested panel{%- endcomment -%}
    {%- for link in menu.links -%}
      {%- if link.links != blank -%}
        {%- capture panel_id -%}mobile-panel-{{ forloop.index0 }}{%- endcapture -%}
        <div id="{{ panel_id }}" class="mobile-menu__panel is-nested">
          <div class="mobile-menu__section is-sticky">
            <button class="mobile-menu__back-button" data-action="close-panel">{% render 'icon', icon: 'arrow-left' %} {{ 'header.navigation.back' | t }}</button>
          </div>
          <div class="mobile-menu__section">
            {%- assign is_mega_menu = false -%}
            {%- assign downcase_title = link.title | downcase | strip -%}
            <ul class="mobile-menu__nav" data-type="menu">
              <li class="mobile-menu__nav-item afa">
                <a href="{{ link.url }}" class="mobile-menu__nav-link text--strong">{{ link.title }}</a>
              </li>
              {%- for sub_link in link.links -%}
              <li class="mobile-menu__nav-item vaww">
                {%- if sub_link.links != blank -%}
                {%- capture panel_id -%}mobile-panel-{{ forloop.parentloop.index0 }}-{{ forloop.index0 }}{%- endcapture -%}
                <button class="mobile-menu__nav-link" data-type="menuitem" aria-haspopup="true" aria-expanded="false" aria-controls="{{ panel_id }}" data-action="open-panel">
                  {{- sub_link.title -}} {%- render 'icon', icon: 'arrow-right' -%}
                </button>
                {%- else -%}
                <a href="{{ sub_link.url }}" class="mobile-menu__nav-link" data-type="menuitem">{{ sub_link.title }}</a>
                {%- endif -%}
              </li>
              {%- endfor -%}
            </ul>
          </div>
        </div>
      {%- endif -%}
    {%- endfor -%}

    {%- comment -%}Finally we need to do the same to create the third level menus{%- endcomment -%}
    {%- for link in menu.links -%}
      {%- if link.links != blank -%}
        {%- assign downcase_title = link.title | downcase -%}
        {%- unless mega_menus contains downcase_title -%}
          {%- for sub_link in link.links -%}
            {%- if sub_link.links != blank -%}
              {%- capture panel_id -%}mobile-panel-{{ forloop.parentloop.index0 }}-{{ forloop.index0 }}{%- endcapture -%}
              <div id="{{ panel_id }}" class="mobile-menu__panel is-nested 123">
                <div class="mobile-menu__section is-sticky">
                  <button class="mobile-menu__back-button" data-action="close-panel">{% render 'icon', icon: 'arrow-left' %} {{ 'header.navigation.back' | t }}</button>
                </div>
                <div class="mobile-menu__section">
                  <ul class="mobile-menu__nav" data-type="menu">
                    <li class="mobile-menu__nav-item mama">
                      <a href="{{ sub_link.url }}" class="mobile-menu__nav-link text--strong">{{ sub_link.title }}</a>
                    </li>
                    {%- for sub_sub_link in sub_link.links -%}
                      <li class="mobile-menu__nav-item lkoa">
                        <a href="{{ sub_sub_link.url }}" class="mobile-menu__nav-link" data-type="menuitem">{{ sub_sub_link.title }}</a>
                      </li>
                    {%- endfor -%}
                  </ul>
                </div>
              </div>
            {%- endif -%}
          {%- endfor -%}
        {%- endunless -%}
      {%- endif -%}
    {%- endfor -%}

    <div id="mobile-panel-1" class="mobile-menu__panel is-nested">
      <div class="mobile-menu__section is-sticky">
        <button class="mobile-menu__back-button" data-action="close-panel"><svg class="icon icon--arrow-left" viewBox="0 0 8 12" role="presentation">
          <path stroke="currentColor" stroke-width="2" d="M6 10L2 6l4-4" fill="none" stroke-linecap="square"></path>
          </svg> Back</button>
      </div>
      <div class="mobile-menu__section">
        <ul class="mobile-menu__nav" data-type="menu">
          {%- form 'localization', id: 'localization_form_header_currency_mobile' -%}
          <li class="mobile-menu__nav-item">
            <button type="submit" name="currency_code" class="mobile-menu__nav-link text--strong" value="USD" {% if 'USD' == form.current_currency.iso_code %}aria-current="true"{% endif %}>
              USD $
            </button>
          </li>
           <li class="mobile-menu__nav-item">
            <button type="submit" name="currency_code" class="mobile-menu__nav-link text--strong" value="CAD" {% if 'CAD' == form.current_currency.iso_code %}aria-current="true"{% endif %}>
              CAD $
            </button>
          </li>
           <li class="mobile-menu__nav-item">
            <button type="submit" name="currency_code" class="mobile-menu__nav-link text--strong" value="EUR" {% if 'EUR' == form.current_currency.iso_code %}aria-current="true"{% endif %}>
              EUR €
            </button>
          </li>
           <li class="mobile-menu__nav-item">
            <button type="submit" name="currency_code" class="mobile-menu__nav-link text--strong" value="GBP" {% if 'GBP' == form.current_currency.iso_code %}aria-current="true"{% endif %}>
              GBP £
            </button>
          </li>
          {%- for currency in form.available_currencies reversed -%}
           {% unless currency.iso_code contains 'USD' or currency.iso_code contains 'CAD' or currency.iso_code contains 'EUR' or currency.iso_code contains 'GBP' %}  
            <li class="mobile-menu__nav-item">
              <button type="submit" name="currency_code" class="mobile-menu__nav-link text--strong" value="{{ currency.iso_code }}" {% if currency.iso_code == form.current_currency.iso_code %}aria-current="true"{% endif %}>
                {{ currency.iso_code }} {% if currency.symbol %}{{ currency.symbol }}{% endif %}
              </button>
            </li>
           {% endunless %}
          {%- endfor -%}
          {%- endform -%}
        </ul>
      </div>
    </div>
    <div id="mobile-panel-2" class="mobile-menu__panel is-nested">
      <div class="mobile-menu__section is-sticky">
        <button class="mobile-menu__back-button" data-action="close-panel"><svg class="icon icon--arrow-left" viewBox="0 0 8 12" role="presentation">
          <path stroke="currentColor" stroke-width="2" d="M6 10L2 6l4-4" fill="none" stroke-linecap="square"></path>
          </svg> Back</button>
      </div>

      <div class="mobile-menu__section">
        <ul class="mobile-menu__nav" data-type="menu">
           {%- form 'localization', id: 'localization_form_header_locale_mobile' -%}
             <li class="mobile-menu__nav-item">
              <button type="submit" name="locale_code" class="mobile-menu__nav-link text--strong" value="en" {% if 'en' == form.current_locale.iso_code %}aria-current="true"{% endif %}>
                English
              </button>
            </li>
            <li class="mobile-menu__nav-item">
              <button type="submit" name="locale_code" class="mobile-menu__nav-link text--strong" value="fr" {% if 'fr' == form.current_locale.iso_code %}aria-current="true"{% endif %}>
                Français
              </button>
            </li>
            <li class="mobile-menu__nav-item">
              <button type="submit" name="locale_code" class="mobile-menu__nav-link text--strong" value="es" {% if 'es' == form.current_locale.iso_code %}aria-current="true"{% endif %}>
                Español
              </button>
            </li>
            <li class="mobile-menu__nav-item">
              <button type="submit" name="locale_code" class="mobile-menu__nav-link text--strong" value="de" {% if 'de' == form.current_locale.iso_code %}aria-current="true"{% endif %}>
                Deutsch
              </button>
            </li>
            <li class="mobile-menu__nav-item">
              <button type="submit" name="locale_code" class="mobile-menu__nav-link text--strong" value="it" {% if 'it' == form.current_locale.iso_code %}aria-current="true"{% endif %}>
                Italiano
              </button>
            </li>
          {%- for locale in form.available_locales -%}
            {% unless locale.iso_code contains 'en' or locale.iso_code contains 'fr' or locale.iso_code contains 'es' or locale.iso_code contains 'de' or locale.iso_code contains 'it' %}
              <li class="mobile-menu__nav-item">
                <button type="submit" name="locale_code" class="mobile-menu__nav-link text--strong" value="{{ locale.iso_code }}" {% if locale.iso_code == form.current_locale.iso_code %}aria-current="true"{% endif %}>
                  {{- locale.endonym_name | capitalize -}}
                </button>
              </li>
            {% endunless %}
          {%- endfor %}
        {% endform %}
        </ul>
      </div>
    </div>

    <div id="mobile-panel-11" class="mobile-menu__panel is-nested monitor-menu">
      {% for block in section.blocks %}
      {% if block.type == 'monitors_menu' %}
      <div class="mobile-menu__section is-sticky">
        <button class="mobile-menu__back-button" data-action="close-panel">
          {%- render 'icon', icon: 'arrow-left' -%} <img src="{{block.settings.m_mob_menu_wht_icon | img_url: '' }}" alt="" loading="lazy"> {{block.settings.title}}
