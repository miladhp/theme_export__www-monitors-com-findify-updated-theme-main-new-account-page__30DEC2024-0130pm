        </div>
        <div class="shop_manufacture">
          {% if block.settings.m_manufa_sec_title != blank %}
          <div class="top_txt">
            <h3>{{ block.settings.m_manufa_sec_title }}</h3>
          </div>
          {% endif %}
          <ul class="cat-items">
            {% if block.settings.m_manufa_name1 != blank %}
            <li class="cat-item">
              <a href="{{ block.settings.m_manufa_url1 }}">{{ block.settings.m_manufa_name1 }}</a>
            </li>
            {% endif %}
            {% if block.settings.m_manufa_name2 != blank %}
            <li class="cat-item">
              <a href="{{ block.settings.m_manufa_url2 }}">{{ block.settings.m_manufa_name2 }}</a>
            </li>
            {% endif %}
            {% if block.settings.m_manufa_name3 != blank %}
            <li class="cat-item">
