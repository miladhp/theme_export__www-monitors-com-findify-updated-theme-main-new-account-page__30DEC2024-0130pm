{% liquid
  assign color = color | downcase
  assign stroke = 'white'
  if color == 'white'
    assign stroke = 'black'
  endif
%}

<svg
  width="14"
  height="11"
  viewBox="0 0 14 11"
  fill="none"
  xmlns="http://www.w3.org/2000/svg"
>
  <path
    d="M1 5.44444L4.69231 9L13 1"
    stroke="{{stroke}}"
    stroke-width="2"
    stroke-linecap="round" />
</svg>
